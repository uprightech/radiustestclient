<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<!-- the hostname of the radius server -->
	<entry key="server.ipaddress">192.168.1.1</entry>
	<!-- the authentication port for the radius server -->
	<entry key="server.authport">1812</entry>
	<!-- the accounting port for the radius server -->
	<entry key="server.acctport">1813</entry>

	<!-- The radius shared secret -->
	<entry key="server.sharedsecret">*wS?U+W6use_wppS</entry>

	<!-- server response timeout in milliseconds -->
	<entry key="server.timeout">1500</entry>

	<!-- AUTHENTICATION PARAMETERS -->

	<!-- username -->
	<entry key="auth.username">user</entry>
	<!-- password -->
	<entry key="auth.password">password@123456</entry>

	<!-- retry count -->
	<entry key="auth.retrycount">6</entry>

	<!-- file containing radius attributes to submit on our request -->
	<entry key="radius.attributes">attrs-full-metal-jacket.xml</entry>

</properties>